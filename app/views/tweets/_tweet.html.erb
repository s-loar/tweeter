<% ##= turbo_frame_tag dom_id(tweet) do %>
  <div class="card card-body">
    <div><%= tweet.message %></div>

    <div class="mt-2">
      <%= button_to "Likes (#{tweet.likes})", tweet_like_path(tweet), method: :post %>
      <%= link_to "Edit", edit_tweet_path(tweet) %>
    </div>
  </div>
<% ##end %>